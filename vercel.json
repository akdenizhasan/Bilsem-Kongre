
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://sites.google.com https://*.google.com;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://sites.google.com"
        }
      ]
    }
  ]
}
